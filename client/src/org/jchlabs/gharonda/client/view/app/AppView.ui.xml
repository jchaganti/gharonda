							<g:MenuItem><ui:msg key="map">Map</ui:msg>
							<g:MenuBar vertical="true">
								<g:MenuItem ui:field="saleMapMenu" styleName="menu_item" width="55px" height="14px"> <ui:msg key="forSale">For Sale</ui:msg>  </g:MenuItem>
								<g:MenuItem ui:field="rentMapMenu" styleName="menu_item" width="55px" height="14px"> <ui:msg key="forRent">For Rent</ui:msg></g:MenuItem>
							</g:MenuBar>
