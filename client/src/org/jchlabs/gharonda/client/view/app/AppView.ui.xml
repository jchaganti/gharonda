								<g:MenuItem ui:field="sellMenu" styleName="menu_item" width="115px" height="14px"> <ui:msg key="selling">Selling</ui:msg></g:MenuItem>
								<g:MenuItem ui:field="rentMenu" styleName="menu_item" width="115px" height="14px"> <ui:msg key="renting">Renting</ui:msg>  </g:MenuItem>
