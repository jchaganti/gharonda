<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.sahibidenev.domain.model">
	<class
		name="Notifierprofiles"
		table="notifierprofiles"
	>
		<meta attribute="sync-DAO">true</meta>

		<id
			name="Id"
			type="integer"
			column="id"
		>
			<generator class="increment"/>
		</id>

		<property
			name="TimeStamp"
			column="timeStamp"
			type="java.lang.Long"
			not-null="false"
			length="19"
		/>
		<property
			name="State"
			column="state"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="City"
			column="city"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Suburb"
			column="suburb"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="PType"
			column="pType"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="SftLVal"
			column="sftLVal"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="SftComparator"
			column="sftComparator"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="SftRVal"
			column="sftRVal"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="PriceLVal"
			column="priceLVal"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="PriceComparator"
			column="priceComparator"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="PriceRVal"
			column="priceRVal"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="RoomNoLVal"
			column="roomNoLVal"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="RoomNoComparator"
			column="roomNoComparator"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="RoomNoRVal"
			column="roomNoRVal"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="BuildDateLVal"
			column="buildDateLVal"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="BuildDateComparator"
			column="buildDateComparator"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="BuildDateRVal"
			column="buildDateRVal"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Amenity1"
			column="amenity1"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Amenity2"
			column="amenity2"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Amenity3"
			column="amenity3"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Amenity4"
			column="amenity4"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Frequency"
			column="frequency"
			type="integer"
			not-null="false"
			length="10"
		/>


	</class>	
</hibernate-mapping>